{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": 3
      }
    ],
    // 使用babel编译ts https://babeljs.io/docs/babel-preset-typescript
    "@babel/preset-typescript",
    [
      "@babel/react",
      {
        /* 自动导入（import）JSX 转换而来的函数 文件无需单独引入React */
        "runtime": "automatic"
      }
    ]
  ]
}